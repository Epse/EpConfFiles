begin
  remote = HP_RC1762307-01
  prog=pulseaudio
  config=volume-down
  button=KEY_VOLUMEDOWN
  repeat=0
end

begin
  remote = HP_RC1762307-01
  prog=pulseaudio
  config=volume-up
  button=KEY_VOLUMEUP
  repeat=0
end

begin
  remote = HP_RC1762307-01
  prog = irexec
  config = pulseaudio-ctl up
  button=KEY_VOLUMEUP
  repeat=0
end

begin
  remote = HP_RC1762307-01
  prog=pulseaudio
  config=mute-toggle
  button=KEY_MUTE
  repeat=0
end

